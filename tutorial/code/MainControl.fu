public static class MainControl {
    public static void Main (string[] args) {
        int x = 3 ;
        if (x < 3) {
            Console.WriteLine("X is small!");
        } else if (x == 3) {
            Console.WriteLine("X is 3!");
        } else {
            Console.WriteLine("X is large!");
        }
		while (x < 10) {
			x = x + 1;
			Console.WriteLine($"X is {x}!");
		}
		for (int i = 0; i < 3; i++) {
			x=x+1;
		}
		Console.WriteLine($"Final value of X is {x}!");
		
    }   
}
